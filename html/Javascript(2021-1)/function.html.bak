  <title>Sum 함수사용</title>
 </head>
 <body>
  <pre>
  <h2> sum 함수 사용<hr>
  <script>
  	function add(a,b) //합계 처리
		{
			var s = a+b;
			return s;}
	function sub(a,b) // 뺄셈 처리
	{
		var t = a-b;
		return t;
	}
	var tot1, tot2, n=5, m=10;
	tot1 = add(n.m);//add함수 호출
	tot2 = sub(n,m);//sub함수 호출
	document.write("tot1="+tot1+"&nbsp"+"tot2="+tot2);
  </script>
  </h2>
 </body>
